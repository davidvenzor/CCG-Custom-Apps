<core:FragmentDefinition	
	xmlns:core="sap.ui.core" 
	xmlns="sap.m">
	<Dialog	id="reexplodeBOM" 
		type="Message"
		state="Information"
		title="{i18n>vchclf_structpnl_reexplode_bom_dialog_title}"
		escapeHandler=".onEscape"
		verticalScrolling="false">
		<beginButton>
			<Button id="reexplodeBOMDialogReexplodeButton" 
					type="Emphasized"
					text="{i18n>vchclf_structpnl_reexplode_bom_dialog_reexplode_button}"
					tooltip="{i18n>vchclf_structpnl_reexplode_bom_dialog_reexplode_button}"
					press=".onReexplodeBOM"/>
		</beginButton>
		<content>
			<Text 
				text="{i18n>vchclf_structpnl_reexplode_bom_dialog_content}"
				tooltip="{i18n>vchclf_structpnl_reexplode_bom_dialog_content}"/>
		</content>
	</Dialog>
</core:FragmentDefinition>
