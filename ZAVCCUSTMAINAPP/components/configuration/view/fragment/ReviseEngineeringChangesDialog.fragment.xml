<core:FragmentDefinition	
	xmlns:core="sap.ui.core" 
	xmlns="sap.m">
	<Dialog	id="reviseEngineeringChanges" 
		type="Message"
		state="Information"
		title="{vchI18n>REVISE_ENGINEERING_CHANGES_DIALOG}"
		contentWidth="432px"
		verticalScrolling="false">
		<beginButton>
			<Button id="okButton" 
			type="Emphasized"
			text="{vchI18n>REVISE_ENGINEERING_CHANGES_OK_BUTTON}"
			press=".onReviseEngineeringChanges"/>
		</beginButton>
		<endButton>
			<Button id="cancelButton" 
			text="{vchI18n>REVISE_ENGINEERING_CHANGES_CANCEL_BUTTON}"/>
		</endButton>
		<content>
			<Text text="{vchI18n>REVISE_ENGINEERING_CHANGES_DIALOG_CONTENT}"/>
		</content>
	</Dialog>
</core:FragmentDefinition>
