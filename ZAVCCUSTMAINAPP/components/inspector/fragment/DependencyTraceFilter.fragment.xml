<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:form="sap.ui.layout.form"
	xmlns:smartfield="sap.ui.comp.smartfield">
	<form:SimpleForm
		editable="true"
		layout="ResponsiveGridLayout"
		labelSpanS="12"
		labelSpanM="12"
		labelSpanL="12"
		labelSpanXL="12">
		<form:content>
			<Label
				text="{i18n>vchclf_trace_filter_dialog_trace_level}"
				tooltip="{i18n>vchclf_trace_filter_dialog_trace_level}"
				labelFor="idTraceLevel"/>
			<Select
				id="idTraceLevel"
				items="{ui>/traceLevelAttributes/items}"
				selectedKey="{ui>/filters/TraceLevel}"
				change="onTraceLevelSelectionChanged">
				<items>
					<core:Item
						key="{ui>key}"
						text="{ui>text}"/>
				</items>
			</Select>
			<Label
				text="{i18n>vchclf_trace_filter_dialog_message_types}"
				tooltip="{i18n>vchclf_trace_filter_dialog_message_types}"
				labelFor="idMessageTypes"/>
			<MultiComboBox
				id="idMessageTypes"
				items="{ui>/messageTypeAttributes/items}"
				selectedKeys="{ui>/filters/MessageType}"
				selectionChange="onMessageTypesSelectionChanged">
				<core:Item
					key="{ui>key}"
					text="{ui>text}"/>
			</MultiComboBox>
			<Label
				text="{i18n>vchclf_trace_filter_dialog_value_assignment_by}"
				tooltip="{i18n>vchclf_trace_filter_dialog_value_assignment_by}"	
				labelFor="idValueAssignmentBy"/>
			<MultiComboBox
				id="idValueAssignmentBy"
				items="{ui>/valueAssignmentByAttributes/items}"
				selectedKeys="{ui>/filters/ValueAssignmentBy}"
				enabled="{ui>/valueAssignmentByAttributes/enabled}">
				<core:Item
					key="{ui>key}"
					text="{ui>text}"/>
			</MultiComboBox>
			<Label
				text="{i18n>vchclf_trace_filter_dialog_characterisitc}"
				tooltip="{i18n>vchclf_trace_filter_dialog_characterisitc}"
				labelFor="idCharacteristic"/>
			<MultiInput
				id="idCharacteristic"
				tokens="{ui>/filters/Characteristic}"
				tokenUpdate="onCsticsTokenUpdate"
				showSuggestion="true"
				filterSuggests="false"
				suggest="onCsticsSuggest"
				valueHelpRequest="onCsticsValueHelpRequest"
				change="onCsticsValueChange"
				submit="onCsticsValueSubmit"
				value="{ui>/characteristicAttributes/value}"
				valueState="{ui>/characteristicAttributes/valueState}"
				valueStateText="{ui>/characteristicAttributes/valueStateText}"
				suggestionRows="{ui>/characteristicAttributes/suggestedRows}"
				autocomplete="false"
				busyIndicatorDelay="0">
				<suggestionColumns>
					<Column
						hAlign="Begin"
						popinDisplay="Inline"
						demandPopin="true">
						<Label
							text="{i18n>vchclf_trace_cstics_value_help_result_col_cstics}"
							tooltip="{i18n>vchclf_trace_cstics_value_help_result_col_cstics}"/>
					</Column>
					<Column
						hAlign="Center"
						popinDisplay="Inline"
						demandPopin="true">
						<Label 
							text="{i18n>vchclf_trace_cstics_value_help_result_col_csticsdesc}"
							tooltip="{i18n>vchclf_trace_cstics_value_help_result_col_csticsdesc}"/>
					</Column>
				</suggestionColumns>
				<suggestionRows>
					<ColumnListItem>
						<cells>
							<Label
								text="{ui>CsticName}"
								tooltip="{ui>CsticName}"/>
							<Label
								text="{ui>Description}"
								tooltip="{ui>Description}"/>
						</cells>
					</ColumnListItem>
				</suggestionRows>
				<tokens>
					<Token
						key="{ui>key}"
						text="{ui>text}"
						tooltip="{ui>text}"/>
				</tokens>
			</MultiInput>
			<Label
				text="{i18n>vchclf_trace_filter_dialog_objdep}"
				tooltip="{i18n>vchclf_trace_filter_dialog_objdep}"
				labelFor="idDependency" />
			<MultiInput
				id="idDependency"
				tokens="{ui>/filters/ObjectDependency}"
				tokenUpdate="onDependencyTokenUpdate"
				showSuggestion="true"
				filterSuggests="false"
				suggest="onDepsSuggest"
				valueHelpRequest="onDependencyValueHelpRequest"
				change="onDepsValueChange"
				submit="onDepsValueSubmit"
				value="{ui>/dependencyAttributes/value}"
				valueState="{ui>/dependencyAttributes/valueState}"
				valueStateText="{ui>/dependencyAttributes/valueStateText}"
				suggestionRows="{ui>/dependencyAttributes/suggestedRows}"
				autocomplete="false"
				busyIndicatorDelay="0">
				<suggestionColumns>
					<Column
						hAlign="Begin"
						popinDisplay="Inline"
						demandPopin="true">
						<Label
							text="{i18n>vchclf_trace_objdep_value_help_result_col_objdep}"
							tooltip="{i18n>vchclf_trace_objdep_value_help_result_col_objdep}"/>
					</Column>
					<Column
						hAlign="Center"
						popinDisplay="Inline"
						demandPopin="true">
						<Label
							text="{i18n>vchclf_trace_objdep_value_help_result_col_objdepdesc}"
							tooltip="{i18n>vchclf_trace_objdep_value_help_result_col_objdepdesc}"/>
					</Column>
				</suggestionColumns>
				<suggestionRows>
					<ColumnListItem>
						<cells>
							<Label
								text="{ui>ObjectDependencyName}"
								tooltip="{ui>ObjectDependencyName}"/>
							<Label
								text="{ui>Description}"
								tooltip="{ui>Description}"/>
						</cells>
					</ColumnListItem>
				</suggestionRows>
				<tokens>
					<Token
						key="{ui>key}"
						text="{ui>text}"
						tooltip="{ui>text}" />
				</tokens>
			</MultiInput>
		</form:content>
	</form:SimpleForm>
</core:FragmentDefinition>
